// Add/Replace some symbols
default partial alphanumeric_keys
xkb_symbols "basic" {
  key <AD01> { [ NoSymbol, NoSymbol, atilde,      Atilde      ] }; // ã Ã
  key <AD02> { [ NoSymbol, NoSymbol, acircumflex, Acircumflex ] }; // â Â
  key <AD11> { [ NoSymbol, NoSymbol, ordfeminine, registered  ] }; // ª ®
  key <AD12> { [ NoSymbol, NoSymbol, masculine,   mu          ] }; // º µ

  key <AC05> { [ NoSymbol, NoSymbol, otilde,      Otilde      ] }; // õ Õ
  key <AC06> { [ NoSymbol, NoSymbol, ocircumflex, Ocircumflex ] }; // ô Ô

  key <AB04> { [ NoSymbol, NoSymbol, ecircumflex, Ecircumflex ] }; // ê Ê
  key <AB07> { [ NoSymbol, NoSymbol, agrave,      Agrave      ] }; // à À
};
